{
  "version": 2,
  "features": {
    "insightsRecommendations": {
      "flag": true,
      "perc": [[0,30]],
      "filters": [
        {
          "name": "agreements.analytics",
          "value": true,
          "cond": "eq"
        },
        {
          "name": "config.server.buildType",
          "value": "ELECTRON",
          "cond": "eq"
        }
      ]
    },
    "cloudSso": {
      "flag": true,
      "perc": [[0,100]],
      "filters": [
        {
          "name": "config.server.buildType",
          "value": "ELECTRON",
          "cond": "eq"
        }
      ],
      "data": {
        "selectPlan": {
          "components": {
            "triggersAndFunctions": [
              {
                "provider": "AWS",
                "regions": ["ap-southeast-1"]
              },
              {
                "provider": "GCP",
                "regions": ["asia-northeast1"]
              }
            ]
          }
        }
      }
    }
  }
}
